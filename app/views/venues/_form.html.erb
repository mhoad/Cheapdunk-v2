<div class="tabbable "> 
  <ul class="nav nav-tabs">
    <li class="active"><a href="#tab1" data-toggle="tab">Main Details</a></li>
    <li><a href="#tab2" data-toggle="tab">Trading Hours</a></li>
  </ul>
   <%= form_for(@venue) do |f| %>
  <div class="tab-content">
    <div class="tab-pane active" id="tab1">
       <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name, html_options = { :class => "span6" } %>

      <%= f.label :street_address %>
      <%= f.text_field :street_address, html_options = { :class => "span6" } %>

      <%= f.label :suburb %>
      <%= f.text_field :suburb, html_options = { :class => "span6" } %>

      <%= f.label :postcode %>
      <%= f.text_field :postcode, html_options = { :class => "span6" } %>

      <%= f.label :phone_number %>
      <%= f.text_field :phone_number, html_options = { :class => "span6" } %>

      <%= f.label :url %>
      <%= f.text_field :url, html_options = { :class => "span6" } %>

      <%= f.label :email %>
      <%= f.text_field :email, html_options = { :class => "span6" } %>

      <%= f.label :description %>
      <%= f.text_area :description, html_options = { :class => "span6", :size => "6x6" }  %>
    </div>
    <%= f.submit "Submit Venue", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>